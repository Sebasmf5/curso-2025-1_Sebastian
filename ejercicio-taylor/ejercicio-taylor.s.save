.data

signo: .double -1.0  		 #Constante -1 para alternar signos
dos: .double 2.0, 2.0		 # Vector constante 2.0 para incrementar fact 
tres: .double 3.0, 3.0 		 #Vector  inicial 3.0 para factorial
fact_actual .double 1.0, 1.0
.text
.globl seno_taylor

seno_taylor:
    #xmm0 funcion (xmm0, rdi)
    #Argumentos
    #xmm0 = x (angulo en radianes)
    #rdi = numero de iteraciones

   movapd %xmm0, %xmm1		#xmm1 = x (primer termino de la serie de taylor)
   movapd %xmm0, %xmm2		#En xmm2 se llevará la suma acumulada Tn-1 + Tn
   movapd %xmm0, %xmm3		#Este registro tendrá en cuenta las potencias x^n * x^2
   movsd  signo(%rip), %xmm4    #Con este registro se intercala el signo
   movapd fact_actual(%rip), %xmm5 #xmm5 = primer factorial   

while_taylor:

   cmp $0, %rdi			#comparacion entre rdi y cero
   je fin

   #Calcular el termino siguiente
   # T = T * (x^2) / ((2n+1)(2n))
   mulpd %xmm0, %xmm3		# xmm3 = x^n*x
   mulpd %xmm0, %xmm3		# xmm3 = x^n * x^2 (listo el numerador)
   


 
